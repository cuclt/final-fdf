<tr>
  <th><%= user.name %></th>
  <th><%= user.email %></th>
  <th><%= user.chatwork_id %></th>
  <th>
    <% unless current_user.is_user? user %>
      <%= link_to  admin_user_path(user.id), title: t("delete"), method: :delete, data: {confirm: t("admin.confirm_del")} do %>
        <span class="glyphicon glyphicon-remove text-danger"></span>
      <% end %>
    <% end %>
  </th>
</tr>
