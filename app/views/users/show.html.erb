<div class="services">
  <div class="container">
    <div class="col-md-12">
      <h3 class="text-center"><%= t "profile" %></h3>
      <hr>
      <div class="col-md-4" >
        <%= image_tag current_user.avatar, class: "avatar" %>
      </div>
      <div class="col-md-6">
        <h3><strong><%= current_user.name %></strong></h3>
        <p><%= current_user.email %></p>
        <p><%= current_user.chatwork_id %></p>
        <%= link_to t("history_order"), "#", class: "btn btn-danger" %>
      </div>
    </div>
  </div>
</div>
