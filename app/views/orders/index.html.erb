<% if @orders.blank? %>
 <div class="events">
    <div class="container">
      <div class="team-heading">
        <h3><%= t "order_emty" %></h3>
      </div>
    </div>
  </div>
<% else %>
  <div class="events">
    <div class="container">
        <table id="shopping-cart-table" class="data-table cart-table">
          <thead>
            <tr class="em-block-title">
              <th class="a-center">
                <span class="nobr"><%= t ".time" %></span>
              </th>
              <th class="a-center">
                <span class="nobr"><%= t ".quantity" %></span>
              </th>
              <th class="a-center">
                <span class="nobr"><%= t "address" %></span>
              </th>
              <th class="a-center">
                <span class="nobr"><%= t ".status" %></span>
              </th>
              <th class="a-center last" colspan="1"><%= t ".subtotal" %></th>
            </tr>
          </thead>
          <tbody>
            <%= render "list_orders" %>
          </tbody>
        </table>
    </div>
  </div>
<% end %>
